ion-view(view-title="videolist")
	ion-content
		ion-list
			ion-item.item-remove-animate.item-avatar.item-icon-right(ng-repeat='clip in cliplist', type='item-text-wrap', ng-click="activate(video)")
				h2 {{clip.video_key}}
				video(width="320" height="240" controls autoplay)
					source(src="{{clip.src}}" type="video/mp4")
				i.icon.ion-chevron-right.icon-accessory
				
